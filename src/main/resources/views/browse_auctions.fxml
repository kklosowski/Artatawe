<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.BrowseAuctionsController">
    <children>
        <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0"
              prefWidth="600.0">
            <children>
                <GridPane alignment="CENTER" maxHeight="100.0" minHeight="100.0" prefHeight="100.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="560.0" minWidth="560.0" prefWidth="560.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints percentHeight="100.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <GridPane maxWidth="-Infinity" minWidth="-Infinity" prefWidth="560.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="140.0"
                                                   prefWidth="140.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="85.0"
                                                   prefWidth="85.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="85.0"
                                                   prefWidth="85.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="85.0"
                                                   prefWidth="85.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="140.0"
                                                   prefWidth="140.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Button fx:id="paintingsFilterButton" maxWidth="85.0" minWidth="85.0"
                                        mnemonicParsing="false" onAction="#paintingsFilter" prefWidth="85.0"
                                        text="Paintings" GridPane.columnIndex="1"/>
                                <Button fx:id="sculpturesFilterButton" maxWidth="85.0" minWidth="85.0"
                                        mnemonicParsing="false" onAction="#sculpturesFilter" prefWidth="85.0"
                                        text="Sculptures" GridPane.columnIndex="2"/>
                                <Button fx:id="allFilterButton" maxWidth="85.0" minWidth="85.0" mnemonicParsing="false"
                                        onAction="#allFilter" prefWidth="85.0" text="All types"
                                        GridPane.columnIndex="3"/>
                                <CheckBox fx:id="onlyMineCheckbox" maxWidth="240.0" minWidth="240.0"
                                          mnemonicParsing="false" onAction="#onlyMine" prefHeight="17.0"
                                          prefWidth="240.0" text="Only auctions I participate in"
                                          GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1"/>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </GridPane>
                    </children>
                    <VBox.margin>
                        <Insets left="10.0" right="10.0"/>
                    </VBox.margin>
                </GridPane>
                <ScrollPane hbarPolicy="NEVER" maxHeight="310.0" maxWidth="560.0" minHeight="310.0" minWidth="560.0"
                            prefHeight="310.0" prefWidth="560.0">
                    <content>
                        <AnchorPane maxWidth="560.0" minWidth="560.0" prefWidth="560.0">
                            <children>
                                <VBox id="auctions" fx:id="auctions" maxWidth="560.0" minHeight="310.0" minWidth="560.0"
                                      prefWidth="560.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </ScrollPane>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</Pane>
